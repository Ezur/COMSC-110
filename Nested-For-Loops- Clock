#include <iostream>
#include <iomanip>
using namespace std;

const int WIDTH = 2;

int main()
{
    cout.fill('0');
	for (int h = 0; h <= 23; h++)
		for (int m = 0; m < 60; m++)
			for (int s = 0; s < 60; s+=10)
				cout << setw(WIDTH) << h << ":"
					 << setw(WIDTH) << m << ":"
					 << setw(WIDTH) << s << endl;

	return 0;
}
