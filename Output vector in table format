#include <iostream>
#include <iomanip>
#include <vector>
using namespace std;

const int WIDTH = 6;
const int COLS = 10;

int main()
{
    srand(time(0));
	vector<int> numbers;
	for (int i = 0; i < 50; i++)
		numbers.push_back(rand() % 1000);

	for (int i = 1; i <= 50; i++)
	{
		cout << setw(WIDTH) << numbers[i];
		if (i % COLS == 0) cout << endl;
	}

    return 0;
