#include <iostream>
#include <vector>
using namespace std;

int main()
{
    srand(time(0));
    vector<int> values;

	for (int i = 0; i < 10; i++)
		values.push_back(rand() % 100);

	for (auto val : values) 
        << val << " ";
	cout << endl;

	values.pop_back();
	values.pop_back();
	for (auto val : values) 
        cout << val << " ";
	cout << endl;

	int val = values[values.size() - 1];
	cout << "val is: " << val << endl;

	cout << "Erasing 3rd & 4th elements: \n";
	values.erase(values.begin() + 2, values.begin() + 4);
	for (auto val : values) 
        cout << val << " ";
	cout << endl;

	cout << "Inserting a new 5th element of 5000:\n";
	values.insert(values.begin() + 5, 5000);
	for (auto val : values) 
        cout << val << " ";
	cout << endl;


	return 0;
}
