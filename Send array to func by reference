#include <iostream>
#include <fstream>
#include <array>
using namespace std;

const int SIZE = 10;

void print_array(array<int, SIZE> &);

int main()
{
    //int numbers[SIZE];
	array<int, SIZE> numbers;
	ifstream fin ("input.txt");

	for (int i = 0; i < SIZE; i++)
		fin >> numbers[i];
	fin.close();

	print_array(numbers);

	for (int i = 0; i < SIZE; i++)
		cout << numbers[i] << " ";
	cout << endl;

    return 0;
}

void print_array(array<int, SIZE> &a)
{
	for (int i = 0; i < SIZE; i++)
	{
		a[i] *= 100;
		cout << a[i] << " ";
	}
	cout << endl;
}
