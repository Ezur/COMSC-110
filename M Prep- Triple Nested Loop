#include <iostream>
using namespace std;
 
int main() {
 
    // welcome
    cout << "*** NESTED LOOP PROGRAM ***\n";
    cout << "This program will output 000-299 in columns using\n";
    cout << "nested loops to print the digits.\n\n";
   
    // looping and output
    for (int i = 0; i < 3; i++)
        for (int j = 0; j < 10; j++)
        {
            for (int k = 0; k < 10; k++ )
                cout << i << j << k << "  ";
            cout << endl;
        }
 
    return 0;
}
